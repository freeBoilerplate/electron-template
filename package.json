{
  "main": "electron/main.js",
  "homepage": "./",
  "name": "templategame",
  "description": "A template that allows you to build games for steam using electron.",
  "version": "0.1.0",
  "private": true,
  "author": "8Byte Studios",
  "dependencies": {
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.1.0",
    "@testing-library/user-event": "12.1.10",
    "electron-is-dev": "2.0.0",
    "electron-store": "8.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "5.2.1",
    "react-scripts": "4.0.3"
  },
  "scripts": {
    "start": "set BROWSER=none && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "electron": "set ELECTRON_START_URL=http://localhost:3000 && electron .",
    "build-electron": "mkdirp build/src && robocopy electron build/electron /s & robocopy src/shared build/src/shared /s",
    "package": "electron-builder build --win -c.extraMetadata.main=build/electron/main.js --publish never"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "14.0.0",
    "electron-builder": "22.11.7",
    "electron-packager": "15.3.0",
    "mkdirp": "^1.0.4",
    "webpack-node-externals": "3.0.0"
  },
  "build": {
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "publish": {
      "provider": "github",
      "repo": "electron-cra-example",
      "owner": "johndyer24"
    }
  }
}
